{
    "name": "Spendix",
    "version": "0.1.0",
    "description": "Keep track of how much money you spend.",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Spendix",
        "default_popup": "popout.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "icons": {
        "16": "image/icon16.png",
        "32": "image/icon32.png",
        "48": "image/icon48.png",
        "128": "image/icon128.png"
    }
}